{
  "name": "@everyprotocol/kind-as",
  "version": "0.1.1",
  "type": "module",
  "files": [
    "bin/",
    "std/",
    "README.md",
    "LICENSE"
  ],
  "bin": {
    "kasc": "./bin/index.js"
  },
  "dependencies": {},
  "devDependencies": {
    "@eslint/js": "^9.26.0",
    "@types/bun": "latest",
    "@types/lodash-es": "^4.17.12",
    "assemblyscript": "^0.27.36",
    "commander": "^13.1.0",
    "eslint": "^9.26.0",
    "keccak256": "^1.0.6",
    "lodash-es": "^4.17.21",
    "typedoc": "^0.28.7",
    "typedoc-plugin-markdown": "^4.6.3",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.32.1"
  },
  "scripts": {
    "clean": "rm -rf bin",
    "check": "tsc --noEmit",
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "build": "bun build src/index.ts --outdir bin --target node",
    "prebuild": "bun run check && bun run lint",
    "prepublishOnly": "bun run build",
    "b": "bun run build",
    "docs": "bun run docs:html && bun run docs:md",
    "docs:md": "bun run prebuild && typedoc --options typedoc.md.json",
    "docs:html": "bun run prebuild && typedoc --options typedoc.html.json"
  },
  "publishConfig": {
    "access": "public"
  }
}
